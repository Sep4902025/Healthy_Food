    },
    "web": {
      "favicon": "./assets/LogoApp.png",
      "output": "static"
    },
    "plugins": ["expo-font"],
    "owner": "tructht",
    "extra": {
      "eas": {
        "projectId": "06e38a55-f8cf-43a2-9401-ea0b16cd09b1"
